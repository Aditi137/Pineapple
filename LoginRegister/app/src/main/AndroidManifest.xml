<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sgc.loginregister">

    <uses-permission android:name="android.permission.INTERNET" />
    "
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".UserAreaActivity" />
        <activity android:name=".ForgetPasswordActivity" />
        <activity android:name=".ReminderActivity" />
        <activity android:name=".alertActivity" />
        <activity android:name=".CreateReminderActivity" />
        <activity android:name=".AccountTypeActivity" />
        <activity android:name=".RemindersViewActivity" />
        <activity android:name=".EditReminderActivity" />
        <activity android:name=".LinkAccounts" />

        <receiver android:name=".Alarm">
            <intent-filter>
                <action android:name="sgc.android.mybroadcast" />
            </intent-filter>
        </receiver>

        <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />

        <activity android:name=".SuperviseeWatchListActivity" />
        <activity android:name=".SuperviseeMain"></activity>
    </application>

</manifest>